{
    "mcpServers": {
        "fooddb": {
            "command": "uv",
            "args": [
                "--directory",
                "/path/to/fooddb",
                "run",
                "food",
                "run-server"
            ],
            "env": {
                "OPENAI_API_KEY": "your-openai-api-key-here"
            }
        }
    }
}